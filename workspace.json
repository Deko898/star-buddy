{
  "version": 1,
  "projects": {
    "star-buddy": {
      "projectType": "application",
      "schematics": {
        "@nrwl/angular:component": {
          "style": "scss"
        }
      },
      "root": "apps/star-buddy",
      "sourceRoot": "apps/star-buddy/src",
      "prefix": "star-buddy",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/apps/star-buddy",
            "index": "apps/star-buddy/src/index.html",
            "main": "apps/star-buddy/src/main.ts",
            "polyfills": "apps/star-buddy/src/polyfills.ts",
            "tsConfig": "apps/star-buddy/tsconfig.app.json",
            "aot": true,
            "assets": [
              "apps/star-buddy/src/favicon.ico",
              "apps/star-buddy/src/assets"
            ],
            "styles": ["apps/star-buddy/src/styles.scss"],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/star-buddy/src/environments/environment.ts",
                  "with": "apps/star-buddy/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "star-buddy:build",
            "proxyConfig": "apps/star-buddy/proxy.conf.json"
          },
          "configurations": {
            "production": {
              "browserTarget": "star-buddy:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "star-buddy:build"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "apps/star-buddy/tsconfig.app.json",
              "apps/star-buddy/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!apps/star-buddy/**/*"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/star-buddy/jest.config.js",
            "tsConfig": "apps/star-buddy/tsconfig.spec.json",
            "passWithNoTests": true,
            "setupFile": "apps/star-buddy/src/test-setup.ts"
          }
        }
      }
    },
    "star-buddy-e2e": {
      "root": "apps/star-buddy-e2e",
      "sourceRoot": "apps/star-buddy-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/star-buddy-e2e/cypress.json",
            "tsConfig": "apps/star-buddy-e2e/tsconfig.e2e.json",
            "devServerTarget": "star-buddy:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "star-buddy:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": ["apps/star-buddy-e2e/tsconfig.e2e.json"],
            "exclude": ["**/node_modules/**", "!apps/star-buddy-e2e/**/*"]
          }
        }
      }
    },
    "api": {
      "root": "apps/api",
      "sourceRoot": "apps/api/src",
      "projectType": "application",
      "prefix": "api",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@nrwl/node:build",
          "options": {
            "outputPath": "dist/apps/api",
            "main": "apps/api/src/main.ts",
            "tsConfig": "apps/api/tsconfig.app.json",
            "assets": ["apps/api/src/assets"]
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false,
              "fileReplacements": [
                {
                  "replace": "apps/api/src/environments/environment.ts",
                  "with": "apps/api/src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@nrwl/node:execute",
          "options": {
            "buildTarget": "api:build"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:lint",
          "options": {
            "linter": "eslint",
            "tsConfig": [
              "apps/api/tsconfig.app.json",
              "apps/api/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!apps/api/**/*"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/api/jest.config.js",
            "tsConfig": "apps/api/tsconfig.spec.json",
            "passWithNoTests": true
          }
        }
      }
    },
